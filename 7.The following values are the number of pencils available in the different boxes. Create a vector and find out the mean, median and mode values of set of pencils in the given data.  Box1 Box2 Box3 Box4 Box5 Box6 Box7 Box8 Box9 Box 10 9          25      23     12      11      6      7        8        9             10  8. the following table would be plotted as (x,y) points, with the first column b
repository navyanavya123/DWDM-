# Given data
pencil_quantities <- c(9, 25, 23, 12, 11, 6, 7, 8, 9, 10)

# Mean calculation
mean_value <- mean(pencil_quantities)

# Median calculation
median_value <- median(pencil_quantities)

# Mode calculation
mode_value <- as.numeric(names(table(pencil_quantities)[table(pencil_quantities) == max(table(pencil_quantities))]))

# Output
print(paste("Mean:", mean_value))
print(paste("Median:", median_value))
print(paste("Mode:", mode_value))
